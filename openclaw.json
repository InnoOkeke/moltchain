{
    "agent": {
        "model": "groq/llama-3.1-70b-versatile",
        "name": "Moltchain"
    },
    "skills": {
        "load": {
            "extraDirs": [
                "./skills"
            ]
        },
        "entries": {
            "moltchain": {
                "enabled": true,
                "env": {
                    "NEYNAR_API_KEY": "${NEYNAR_API_KEY}",
                    "PRIVATE_KEY": "${PRIVATE_KEY}",
                    "FARCASTER_SIGNER_UUID": "${FARCASTER_SIGNER_UUID}"
                }
            },
            "moltbook": {
                "enabled": true,
                "env": {
                    "MOLTBOOK_API_KEY": "${MOLTBOOK_API_KEY}"
                }
            }
        }
    },
    "automation": {
        "cron": [
            {
                "schedule": "*/10 * * * *",
                "message": "Check Farcaster for trending narratives. If you find a strong narrative (strength >= 7), deploy a token via Clanker and announce on Farcaster."
            },
            {
                "schedule": "*/30 * * * *",
                "message": "Track engagement metrics on deployed contracts. Check transfer counts, holder counts, and social mentions. Post an update to Moltbook if there's interesting activity."
            },
            {
                "schedule": "0 * * * *",
                "message": "Review all deployments older than 7 days. If any have less than 10 transfers, announce a sunset on Farcaster."
            }
        ]
    },
    "channels": {
        "farcaster": {
            "enabled": true
        }
    }
}